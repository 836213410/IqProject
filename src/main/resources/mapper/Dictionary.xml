<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.rt.mapper.DictionaryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.rt.vo.Dictionary">
        <id column="dictionaryid" property="dictionaryid" />
        <result column="dictionaryname" property="dictionaryname" />
        <result column="createdate" property="createdate" />
        <result column="sort" property="sort" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        dictionaryid, dictionaryname, createdate, sort
    </sql>

</mapper>
