<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.rt.mapper.AnsweroptionsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.rt.vo.Answeroptions">
        <id column="optionid" property="optionid" />
        <result column="questionid" property="questionid" />
        <result column="optionidcontext" property="optionidcontext" />
        <result column="sort" property="sort" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        optionid, questionid, optionidcontext, sort
    </sql>

</mapper>
